{
  "$schema": "http://json-schema.org/schema#",
  "title": "Example",
  "description": "This is an example payload schema.",
  "type": "object",
  "properties": {
    "emails": {
      "description": "Array of email addresses for this user",
      "type": "array",
      "items": {
        "type": "string",
        "format": "email"
      }
    },
    "event": {
      "description": "Event that the user performed.",
      "type": "string"
    },
    "properties": {
      "description": "Properties of the user",
      "type": "object"
    }
  },
  "required": [
    "emails",
    "event"
  ]
}
