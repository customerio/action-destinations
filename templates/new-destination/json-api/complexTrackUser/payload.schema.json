{
  "$schema": "http://json-schema.org/schema#",
  "title": "Simple JSON Template: Complex track user",
  "description": "Track a user and annotate properties with trivia",
  "type": "object",
  "properties": {
    "email": {
      "description": "Email address that uniquely identifies the user.",
      "type": "string",
      "format": "email"
    },
    "event": {
      "description": "Event that the user performed.",
      "type": "string"
    },
    "number": {
      "description": "Number to get trivia for.",
      "type": "number"
    },
    "properties": {
      "description": "Properties of the user",
      "type": "object"
    }
  },
  "required": ["email", "event", "number"]
}
