{
  "name": "@segment/destination-actions",
  "description": "Destination Actions engine and definitions.",
  "version": "1.0.0-9",
  "repository": {
    "type": "git",
    "url": "https://github.com/segmentio/fab-5-engine",
    "directory": "packages/destination-actions"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "engines": {
    "node": ">=10"
  },
  "engineStrict": true,
  "license": "UNLICENSED",
  "publishConfig": {
    "access": "restricted",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "yarn clean && yarn tsc -b tsconfig.build.json",
    "build:watch": "tsc --watch --preserveWatchOutput",
    "clean": "tsc -b tsconfig.build.json --clean",
    "postclean": "rm -rf dist",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/json-schema": "^7.0.7",
    "@types/lodash": "^4.14.168",
    "@types/mustache": "^4.1.0",
    "@types/uuid": "^8.3.0",
    "jest": "^26.6.3",
    "nock": "^13.0.4"
  },
  "dependencies": {
    "@segment/ajv-human-errors": "^1.1.0",
    "@segment/fab5-subscriptions": "^1.0.0",
    "aggregate-error": "^3.1.0",
    "ajv": "^6.12.4",
    "dayjs": "^1.10.3",
    "got": "^10.0.0",
    "http-errors": "^1.8.0",
    "jsonpath-plus": "^4.0.0",
    "lodash": "^4.17.20",
    "mustache": "^4.1.0",
    "node-cache": "^5.1.2",
    "ts-custom-error": "^3.2.0",
    "uuid": "^8.3.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup-after-env.ts"
    ],
    "forceExit": true
  }
}
