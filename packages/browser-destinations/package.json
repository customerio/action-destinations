{
  "name": "@segment/browser-destinations",
  "version": "2.1.0",
  "description": "Action based browser destinations",
  "author": "Netto Farah",
  "private": true,
  "scripts": {
    "build": "yarn clean && yarn build-web",
    "build-web": "NODE_ENV=production yarn webpack -c webpack.config.js",
    "deploy": "yarn build-web && aws-okta exec stage-privileged -- aws s3 sync ./dist/web/ s3://ajs-next-integrations/fab-5",
    "clean": "tsc -b tsconfig.build.json --clean",
    "postclean": "rm -rf dist",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@segment/actions-core": "^2.0.2",
    "@segment/analytics-next": "^1.13.3",
    "@segment/fab5-subscriptions": "^2.0.1",
    "dayjs": "^1.10.3",
    "tslib": "^2.1.0",
    "vm-browserify": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/amplitude-js": "^7.0.1",
    "@types/intercom-web": "^2.8.11",
    "@types/jest": "^26.0.20",
    "@types/jsdom": "^16.2.6",
    "babel-jest": "^26.6.3",
    "compression-webpack-plugin": "^7.1.2",
    "fs-extra": "^9.1.0",
    "globby": "^11.0.2",
    "jest": "^26.6.3",
    "jsdom": "^16.4.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.17",
    "webpack": "^5.18.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.4.0"
  },
  "jest": {
    "forceExit": true,
    "moduleNameMapper": {
      "@segment/actions-core": "<rootDir>/../core/src/index.ts"
    },
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!@segment/analytics-next).+\\.js$"
    ]
  }
}
