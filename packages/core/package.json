{
  "name": "@segment/actions-core",
  "description": "Core runtime for Destinations Actions.",
  "version": "1.3.0-next.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/segmentio/fab-5-engine",
    "directory": "packages/core"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": "./dist/cjs/index.js",
    "./": "./dist/cjs/"
  },
  "types": "dist/esm/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "package.json"
  ],
  "engines": {
    "node": ">=10.20"
  },
  "engineStrict": true,
  "license": "UNLICENSED",
  "publishConfig": {
    "access": "restricted",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "yarn clean && yarn build:cjs && yarn build:esm",
    "build:cjs": "yarn tsc -p tsconfig.build.json -m commonjs --outDir dist/cjs",
    "build:esm": "yarn tsc -p tsconfig.build.json -m es2015 --outDir dist/esm",
    "clean": "tsc -b tsconfig.build.json --clean",
    "postclean": "rm -rf dist",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/json-schema": "^7.0.7",
    "@types/lodash": "^4.14.168",
    "@types/uuid": "^8.3.0",
    "jest": "^26.6.3",
    "lodash": "^4.17.20",
    "nock": "^13.0.4"
  },
  "dependencies": {
    "@segment/ajv-human-errors": "^1.1.0",
    "@segment/fab5-subscriptions": "^1.0.0",
    "aggregate-error": "^3.1.0",
    "ajv": "^6.12.4",
    "dayjs": "^1.10.3",
    "got": "^10.0.0",
    "http-errors": "^1.8.0",
    "node-cache": "^5.1.2",
    "ts-custom-error": "^3.2.0",
    "uuid": "^8.3.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup-after-env.ts"
    ],
    "forceExit": true
  }
}
